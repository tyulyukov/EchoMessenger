<Application x:Class="EchoMessenger.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:EchoMessenger"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <BitmapImage x:Key="UserImage" UriSource="/resources/user.png" />
        <BitmapImage x:Key="SearchImage" UriSource="/resources/search.png" />
        <BitmapImage x:Key="AttachmentImage" UriSource="/resources/attachment.png" />
        <BitmapImage x:Key="SendImage" UriSource="/resources/attachment.png" />
    </Application.Resources>
</Application>
